{
	"pipeline": {
		"inputs": [
			{"{{files.name}}": {
				"NameA": "{{filesnote}}",
				"NameB": "{{files}}",
				"NameC": "{{files...}}",
				"NameD": "{{nofiles.test}}",
				"NameE": "{{files.nope}}",
				"Name": "{{files.name}}",
				"Listen": "{{files.test}}",
				"Port": 24224
			}},
			{"tail": {
				"Name": "tail",
				"Tag": "tail.01",
				"Path": "/var/system.log"
			}}
		]
		"outputs": [
			{"s3": {
				"Name": "s3",
				"Match": "*",
				"bucket": "{{files.bucket}}"
			}}
		]
	}
}
